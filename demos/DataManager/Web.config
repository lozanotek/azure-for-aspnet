<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appSettings>
        <add key="orwell.ApiUrl" value="http://localhost:8083" />
        <add key="orwell.HubUrl" value="http://localhost:8082/" />
    </appSettings>
    <connectionStrings>
        <!--
            If you want to use the section below, uncomment and enter your values.
            Otherwise it will read a User environment level with the same key for the value.
            
            This is done to to keep the information from being publicly accessible in GitHub.
        -->
        <!--<add name="orwell.Database" connectionString="" providerName="System.Data.SqlClient" />-->
    </connectionStrings>
    <system.web>
        <compilation debug="true" targetFramework="4.5" />
        <httpRuntime targetFramework="4.5" />
        <customErrors mode="Off" />
    </system.web>
    <runtime>
        <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
            <dependentAssembly>
                <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
                <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
            </dependentAssembly>
        </assemblyBinding>
    </runtime>
</configuration>
